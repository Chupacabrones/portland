        <arguments>
          <argument>
            <kind>1</kind>
            <string>///create sprites 
surf = surface_create(1024, 512);
surface_set_target(surf);
draw_clear_alpha(c_black, 0);
draw_sprite(spr_fatchar, 0, 0, 0);


spr_idle = sprite_create_from_surface(surf
    , 32, 0
    , 32, 32
    , false
    , false
    , 16, 16);
    
i = 0;
yp = 0;
while yp  &lt; 128 {
    xp = 0;
    frame = 0;
    while xp &lt; 128 {
    
        //spr_down 
        if yp == 0 {
            if xp == 0 {
                spr_down = sprite_create_from_surface(surf
                    , xp, yp
                    , 32, 32
                    , false
                    , false
                    , 16, 16); 
            } else {
                frame ++;
                sprite_add_from_surface(spr_down, surf
                                        , xp, yp
                                        , 32, 32
                                        , false, false
                                        );
            }            
        }
    
        //spr_left 
        if yp == 32 {
            if xp == 0 {
                spr_left = sprite_create_from_surface(surf
                    , xp, yp
                    , 32, 32
                    , false
                    , false
                    , 16, 16); 
            } else {
                frame ++;
                sprite_add_from_surface(spr_left, surf
                                        , xp, yp
                                        , 32, 32
                                        , false, false
                                        );
            }            
        }
    
        //spr_right
        if yp == 64 {
            if xp == 0 {
                spr_right = sprite_create_from_surface(surf
                    , xp, yp
                    , 32, 32
                    , false
                    , false
                    , 16, 16); 
            } else {
                frame ++;
                sprite_add_from_surface(spr_right, surf
                                        , xp, yp
                                        , 32, 32
                                        , false, false
                                        );
            }            
        }
    
        //spr_up
        if yp == 96 {
            if xp == 0 {
                spr_up = sprite_create_from_surface(surf
                    , xp, yp
                    , 32, 32
                    , false
                    , false
                    , 16, 16); 
            } else {
                frame ++;
                sprite_add_from_surface(spr_up, surf
                                        , xp, yp
                                        , 32, 32
                                        , false, false
                                        );
            }            
        }          
            
        xp += 32;                
        i ++; 
    }
    yp += 32;
}

surface_reset_target();
surface_free(surf);

</string>
          </argument>
        </arguments>
      </action>
